  date = {2014-01-06},
  file = {3880998.epub:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/3C26IJUI/3880998.epub:application/epub+zip;fninf-07-00048.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/VQVMQN5Z/fninf-07-00048.pdf:application/pdf}
