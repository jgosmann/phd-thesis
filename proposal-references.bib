  keywords = {Diazepam,memory,Memory retrieval,Memory storage,Pharmacology/Toxicology,Psychiatry,Scopolamine,Subjective moods},
  file = {Ghoneim_Mewaldt_1975_Effects of diazepam and scopolamine on storage, retrieval and organizational.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/SSQ7DCUS/Ghoneim_Mewaldt_1975_Effects of diazepam and scopolamine on storage, retrieval and organizational.pdf:application/pdf;Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/5N5G856T/BF00428903.html:text/html}
