  keywords = {Auto-associative memory,Holographic reduced representation,neural engineering framework,Vector symbolic architectures,verified},
  file = {ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/FEGQMRC6/S1389041710000525.html:text/html;Stewart et al_2011_A biologically realistic cleanup memory.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/FAS55WM7/Stewart et al_2011_A biologically realistic cleanup memory.pdf:application/pdf}
