  urldate = {2015-01-30},
  date = {2012-11-20},
  pages = {19462--19467},
  langid = {english},
  keywords = {episodic memory,pattern completion,Place cells},
  file = {Mankin et al_2012_Neuronal code for extended time in the hippocampus.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/T9FJMN7B/Mankin et al_2012_Neuronal code for extended time in the hippocampus.pdf:application/pdf;Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/8WSEEKCQ/19462.html:text/html}
