@article{Abel2013,
  title = {Sleep, Plasticity and Memory from Molecules to Whole-Brain Networks},
  volume = {23},
  issn = {0960-9822},
  url = {http://www.cell.com/article/S0960982213008488/abstract},
  doi = {10.1016/j.cub.2013.07.025},
  abstract = {Despite the ubiquity of sleep across phylogeny, its function remains elusive. In this review, we consider one compelling candidate: brain plasticity associated with memory processing. Focusing largely on hippocampus-dependent memory in rodents and humans, we describe molecular, cellular, network, whole-brain and behavioral evidence establishing a role for sleep both in preparation for initial memory encoding, and in the subsequent offline consolidation of memory. Sleep and sleep deprivation bidirectionally alter molecular signaling pathways that regulate synaptic strength and control plasticity-related gene transcription and protein translation. At the cellular level, sleep deprivation impairs cellular excitability necessary for inducing synaptic potentiation and accelerates the decay of long-lasting forms of synaptic plasticity. In contrast, rapid eye movement (REM) and non-rapid eye movement (NREM) sleep enhance previously induced synaptic potentiation, although synaptic de-potentiation during sleep has also been observed. Beyond single cell dynamics, large-scale cell ensembles express coordinated replay of prior learning-related firing patterns during subsequent NREM sleep. At the whole-brain level, somewhat analogous learning-associated hippocampal (re)activation during NREM sleep has been reported in humans. Moreover, the same cortical NREM oscillations associated with replay in rodents also promote human hippocampal memory consolidation, and this process can be manipulated using exogenous reactivation cues during sleep. Mirroring molecular findings in rodents, specific NREM sleep oscillations before encoding refresh human hippocampal learning capacity, while deprivation of sleep conversely impairs subsequent hippocampal activity and associated encoding. Together, these cross-descriptive level findings demonstrate that the unique neurobiology of sleep exerts powerful effects on molecular, cellular and network mechanisms of plasticity that govern both initial learning and subsequent long-term memory consolidation.},
  timestamp = {2015-01-05 14:29:20},
  eprinttype = {pmid},
  eprint = {24028961},
  number = {17},
  journaltitle = {Current Biology},
  author = {Abel, Ted and Havekes, Robbert and Saletin, Jared M. and Walker, Matthew P.},
  urldate = {2015-01-05},
  date = {2013-09-09},
  pages = {R774--R788},
  langid = {english},
  file = {Abel et al_2013_Sleep, Plasticity and Memory from Molecules to Whole-Brain Networks.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/C74UQX9S/Abel et al_2013_Sleep, Plasticity and Memory from Molecules to Whole-Brain Networks.pdf:application/pdf;Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/I255WSFH/S0960-9822(13)00848-8.html:text/html}
}

@article{Fogel2011,
  title = {The function of the sleep spindle: A physiological index of intelligence and a mechanism for sleep-dependent memory consolidation},
  volume = {35},
  issn = {0149-7634},
  url = {http://www.sciencedirect.com/science/article/pii/S0149763410002046},
  doi = {10.1016/j.neubiorev.2010.12.003},
  shorttitle = {The function of the sleep spindle},
  abstract = {Until recently, the electrophysiological mechanisms involved in strengthening new memories into a more permanent form during sleep have been largely unknown. The sleep spindle is an event in the electroencephalogram (EEG) characterizing Stage 2 sleep. Sleep spindles may reflect, at the electrophysiological level, an ideal mechanism for inducing long-term synaptic changes in the neocortex. Recent evidence suggests the spindle is highly correlated with tests of intellectual ability (e.g.; IQ tests) and may serve as a physiological index of intelligence. Further, spindles increase in number and duration in sleep following new learning and are correlated with performance improvements. Spindle density and sigma (14–16 Hz) spectral power have been found to be positively correlated with performance following a daytime nap, and animal studies suggest the spindle is involved in a hippocampal–neocortical dialogue necessary for memory consolidation. The findings reviewed here collectively provide a compelling body of evidence that the function of the sleep spindle is related to intellectual ability and memory consolidation.},
  timestamp = {2015-01-05 14:36:32},
  number = {5},
  journaltitle = {Neuroscience \& Biobehavioral Reviews},
  shortjournal = {Neuroscience \& Biobehavioral Reviews},
  author = {Fogel, Stuart M. and Smith, Carlyle T.},
  urldate = {2015-01-05},
  date = {2011-04},
  pages = {1154--1165},
  keywords = {Consolidation,Intelligence,Learning,memory,Non-REM,sleep,Spindles},
  file = {Fogel_Smith_2011_The function of the sleep spindle.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/VKNGMEMQ/Fogel_Smith_2011_The function of the sleep spindle.pdf:application/pdf}
}

@article{Robertson2004,
  title = {Current concepts in procedural consolidation},
  volume = {5},
