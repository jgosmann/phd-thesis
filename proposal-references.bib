  file = {Fletcher_Henson_2001_Frontal lobes and human memory.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/NJWKS5WE/Fletcher_Henson_2001_Frontal lobes and human memory.pdf:application/pdf;Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/CSWKHMD4/849.html:text/html}
