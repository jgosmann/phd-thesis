  pages = {1172--1190},
  keywords = {Current source density,Dentate gyrus,entorhinal cortex,Region CA1,Region CA3,Single unit recording,Spatial alternation,Theta phase precession},
  file = {Hasselmo_Eichenbaum_2005_Hippocampal mechanisms for the context-dependent retrieval of episodes.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/E3DQSICG/Hasselmo_Eichenbaum_2005_Hippocampal mechanisms for the context-dependent retrieval of episodes.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/MNX2IE8G/S0893608005002005.html:text/html}
