  volume = {36},
  issn = {0010-9452},
  url = {http://www.sciencedirect.com/science/article/pii/S0010945208708438},
  doi = {10.1016/S0010-9452(08)70843-8},
  shorttitle = {Perceptual and Mnemonic Matching-To-Sample in Humans},
  abstract = {Two questions were addressed by the present study. The first was whether the previously reported item recognition deficit which is shown by amnesic patients may be due to a perceptual rather than a memory deficit. To address this question a group of amnesic patients were tested on a 14-choice forced-choice visual item recognition test which included a “simultaneous” condition in which the sample remained visible during the matching decision and a zero second delay. Eacott, Gaffan and Murray (1994) have reported an impairment in simultaneous matching-to-sample following perirhinal damage in monkeys. In our amnesic patients, a deficit was found only after filled delays of 10 seconds or longer and this was also the case for a subgroup of patients whose damage included the perirhinal cortex. The second question, which arose from the model of Aggleton and Brown (1999), was whether performance on the DMS task would remain intact following selective damage to the hippocampus. We tested a patient with bilateral damage to the hippocampus on the 14-choice DMS task and found that her performance was not significantly impaired at delays of up to 30 seconds.},
  timestamp = {2015-02-03 20:46:39},
  number = {3},
  journaltitle = {Cortex},
  shortjournal = {Cortex},
  author = {Holdstock, J. S. and Gutnikov, S. A. and Gaffan, David and Mayes, A. R.},
  urldate = {2015-02-03},
  date = {2000},
  pages = {301--322},
  keywords = {amnesia,Hippocampus,item recognition,Perirhinal cortex},
  file = {Holdstock et al_2000_Perceptual and Mnemonic Matching-To-Sample in Humans.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/7Q4QRZH4/Holdstock et al_2000_Perceptual and Mnemonic Matching-To-Sample in Humans.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/4W93ACFR/S0010945208708438.html:text/html}
}

@article{Aggleton1992,
  title = {The Performance of Postencephalitic Amnesic Subjects on Two Behavioural Tests of Memory: Concurrent Discrimination Learning and Delayed Matching-To-Sample},
  volume = {28},
  issn = {0010-9452},
  url = {http://www.sciencedirect.com/science/article/pii/S0010945213801463},
  doi = {10.1016/S0010-9452(13)80146-3},
  shorttitle = {The Performance of Postencephalitic Amnesic Subjects on Two Behavioural Tests of Memory},
  abstract = {The performance of a group of three postencephalitic subjects with anterograde amnesia was examined on a series of concurrent visual discrimination problems and on a test of visual recognition, delayed matching-to-sample. These tests were chosen as they have been used to assess experimental models of anterograde amnesia in nonhuman primates. In comparison with a group of normal subjects the postencephalitic group were impaired on the more difficult concurrent discrimination problems. They also performed poorly on the matching-to-sample task when given lists of items to remember or given increased retention intervals. The pattern of performance of the postencephalitic group matched closely that of a group of Korsakoff subjects, indicating that these behavioural tests are equally sensitive to different types of anterograde amnesia.},
