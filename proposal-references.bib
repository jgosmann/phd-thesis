@article{Sullivan1991,
  title = {Double dissociation of short-term and long-term memory for nonverbal material in Parkinson's disease and global amnesia},
  volume = {114},
  rights = {© Oxford University Press},
  issn = {0006-8950, 1460-2156},
  url = {http://brain.oxfordjournals.org/content/114/2/893},
  doi = {10.1093/brain/114.2.893},
  abstract = {The traditional concept of memory disorder is deficiency of the long-term (LTM) but not short-term (STM) component of memory. STM impairment with LTM sparing is seldom reported. The present study investigated STM and LTM for nonverbal material in three neurological conditions associated with memory impairment: bilateral medial temporal lobe lesions (patient H.M.), Parkinson's disease (PD) and Alzheimer's disease (AD). Subjects received 3 tests of nonverbal memory: forward block span, immediate and delayed recall of the Wechsler Memory Scale drawings, and immediate and delayed recognition of abstract designs. Compared with the normal control group, the patient groups displayed different patterns of sparing and loss of the two components of memory: in PD, only STM was impaired; in medial temporal lobe amnesia, only LTM was impaired; and in AD, STM and LTM were both impaired. The contrasting patterns of sparing and loss of STM and LTM in PD and global amnesia were present for both recognition and recall. These results provide evidence that STM and LTM are dissociable processes and are served by separate neurological systems: STM depends upon intact corticostriatal systems, whereas LTM depends upon intact medial temporal lobe systems.},
  timestamp = {2015-05-04 22:19:22},
  eprinttype = {pmid},
  eprint = {2043956},
  number = {2},
  journaltitle = {Brain},
  author = {Sullivan, Edith V. and Sagar, Harvey J.},
  urldate = {2015-04-27},
  date = {1991-04-01},
  pages = {893--906},
  langid = {english},
  file = {Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/8X3HZTFP/893.html:text/html;Sullivan_Sagar_1991_Double Dissociation of Short-Term and Long-Term Memory for Nonverbal Material.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/5AA472PR/Sullivan_Sagar_1991_Double Dissociation of Short-Term and Long-Term Memory for Nonverbal Material.pdf:application/pdf}
}

@article{Baddeley1970,
  title = {Amnesia and the distinction between long- and short-term memory},
  volume = {9},
  issn = {0022-5371},
  url = {http://www.sciencedirect.com/science/article/pii/S0022537170800482},
  doi = {10.1016/S0022-5371(70)80048-2},
  abstract = {Evidence for a dichotomy between long-term memory (LTM) and short-term memory (STM) comes from: (a) amnesic patients with a normal digit span but defective LTM, and (b) tasks comprising two components, one labile (STM) and the other stable (LTM). This study examines the compatibility of (a) and (b) by comparing the performance of amnesic and control Ss on immediate and delayed free recall, the Peterson short-term forgetting task, development of PI in STM, minimal paired-associate learning, digit span, and the Hebb repeated digit-sequence technique. Results suggest that amnesic Ss have normal STM but defective LTM. There is some evidence of a stable component in certain STM tasks, on which amnesic Ss are also unimpaired. Implications for the dichotomy between STM and LTM are discussed.},
  timestamp = {2015-04-30 16:37:21},
  number = {2},
  journaltitle = {Journal of Verbal Learning and Verbal Behavior},
  shortjournal = {Journal of Verbal Learning and Verbal Behavior},
  author = {Baddeley, Alan D. and Warrington, Elizabeth K.},
  urldate = {2015-04-30},
  date = {1970-04},
  pages = {176--189},
  file = {Baddeley_Warrington_1970_Amnesia and the distinction between long- and short-term memory.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/NGP7SWZK/Baddeley_Warrington_1970_Amnesia and the distinction between long- and short-term memory.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/D9UBEXEX/S0022537170800482.html:text/html}
}

@book{Miyake1999,
  title = {Models of Working Memory: Mechanisms of Active Maintenance and Executive Control},
  isbn = {9780521587211},
  shorttitle = {Models of Working Memory},
  abstract = {Working memory is currently a "hot" topic in cognitive psychology and neuroscience. Because of their radically different scopes and emphases, however, comparing different models and theories and understanding how they relate to one another has been a difficult task. This work offers a much-needed forum for systematically comparing and contrasting existing models of working memory. It does so by asking each contributor to address the same comprehensive set of important theoretical questions on working memory. The answers to these questions elucidate the emerging general consensus on the nature of working memory among different theorists and clarify incompatible theoretical claims that must be resolved in future research. As such, this volume serves not only as a milestone that documents the state of the art in the field, but also as a theoretical guidebook that will promote new lines of research and more precise and comprehensive models of working memory.},
  pagetotal = {532},
  timestamp = {2015-01-20 22:35:24},
  publisher = {Cambridge University Press},
  author = {Miyake, Akira and Shah, Priti},
  date = {1999-04-13},
  langid = {english},
  keywords = {Computers / Intelligence (AI) \& Semantics,Language Arts \& Disciplines / Linguistics / General,Psychology / Cognitive Psychology,Psychology / Cognitive Psychology \& Cognition}
}

