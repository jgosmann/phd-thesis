@article{Averbeck2002,
  title = {Parallel processing of serial movements in prefrontal cortex},
  volume = {99},
  issn = {0027-8424, 1091-6490},
  url = {http://www.pnas.org/content/99/20/13172},
  doi = {10.1073/pnas.162485599},
  abstract = {A key idea in Lashley's formulation of the problem of serial order in behavior is the postulated neural representation of all serial elements before the action begins. We studied this question by recording the activity of individual neurons simultaneously in small ensembles in prefrontal cortex while monkeys copied geometrical shapes shown on a screen. Monkeys drew the shapes as sequences of movement segments, and these segments were associated with distinct patterns of neuronal ensemble activity. Here we show that these patterns were present during the time preceding the actual drawing. The rank of the strength of representation of a segment in the neuronal population during this time, as assessed by discriminant analysis, predicted the serial position of the segment in the motor sequence. An analysis of errors in copying and their neural correlates supplied additional evidence for this code and provided a neural basis for Lashley's hypothesis that errors in motor sequences would be most likely to occur when executing elements that had prior representations of nearly equal strength.},
  timestamp = {2014-08-11 18:43:22},
