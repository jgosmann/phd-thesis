  date = {2007-09-01},
  pages = {i41--i50},
  langid = {english},
  keywords = {\_tablet,memory,monkey,multi-item,order,sequence,temporal},
  file = {Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/WHEZ2VGT/i41.html:text/html;Warden_Miller_2007_The Representation of Multiple Objects in Prefrontal Neuronal Delay Activity.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/MMD5UBI5/Warden_Miller_2007_The Representation of Multiple Objects in Prefrontal Neuronal Delay Activity.pdf:application/pdf}
