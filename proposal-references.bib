@article{Ranganath2005a,
  title = {Working Memory Maintenance Contributes to Long-term Memory Formation: Neural and Behavioral Evidence},
  volume = {17},
  issn = {0898-929X},
  url = {http://dx.doi.org/10.1162/0898929054475118},
  doi = {10.1162/0898929054475118},
  shorttitle = {Working Memory Maintenance Contributes to Long-term Memory Formation},
  abstract = {Theories of human memory have led to conflicting views regarding the relationship between working memory (WM) maintenance and episodic long-term memory (LTM) formation. Here, we tested the prediction that WM maintenance operates in two stages, and that processing during the initial stage of WM maintenance promotes successful LTM formation. Results from a functional magnetic resonance imaging study showed that activity in the dorsolateral prefrontal cortex and hippocampus during the initial stage of WM maintenance was predictive of subsequent LTM performance. In a behavioral experiment, we demonstrated that interfering with processing during the initial stage of WM maintenance impaired LTM formation. These results demonstrate that processing during the initial stage of WM maintenance directly contributes to successful LTM formation, and that this effect is mediated by a network that includes the dorsolateral prefrontal cortex and the hippocampus.},
  timestamp = {2015-02-03 20:39:42},
  number = {7},
  journaltitle = {Journal of Cognitive Neuroscience},
