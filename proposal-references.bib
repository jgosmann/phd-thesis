@article{Henson1998,
  title = {Short-term memory for serial order: the start-end model},
  volume = {36},
  issn = {0010-0285},
  url = {http://www.sciencedirect.com/science/article/pii/S0010028598906855},
  doi = {10.1006/cogp.1998.0685},
  shorttitle = {Short-Term Memory for Serial Order},
  abstract = {Three solutions to the problem of serial order can be identified: chaining, ordinal and positional theories. Error patterns in serial recall from short-term memory fail to support chaining theories, yet provide unequivocal evidence for positional theories. In a new model of short-term memory, the Start-End Model (SEM), the positions of items in a sequence are coded relative to the start and end of that sequence. Simulations confirm SEM's ability to capture the main phenomena in serial recall, such as the effects of primacy, recency, list length, grouping, modality, redundant suffices, proactive interference, retention interval, and phonological similarity. Moreover, SEM is the first model to capture the complete pattern of errors, including transpositions, repetitions, omissions, intrusions, confusions, and, in particular, positional errors between groups and between trials. Unlike other positional models however, SEM predicts that positional errors will maintain relative rather than absolute position, in agreement with recent experiments (Henson, 1977).},
  timestamp = {2015-05-04 21:07:05},
  number = {2},
  journaltitle = {Cognitive Psychology},
  shortjournal = {Cognitive Psychology},
  author = {Henson, Richard N. A.},
  urldate = {2015-01-27},
  date = {1998-07},
  pages = {73--137},
  file = {Henson_1998_Short-Term Memory for Serial Order.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/8N722WX6/Henson_1998_Short-Term Memory for Serial Order.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/3AH7B55Z/S0010028598906855.html:text/html}
}

