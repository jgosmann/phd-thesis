  file = {Marshall_Born_2007_The contribution of sleep to hippocampus-dependent memory consolidation.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/J8G9T38H/Marshall_Born_2007_The contribution of sleep to hippocampus-dependent memory consolidation.pdf:application/pdf;Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/RQ2A7KHQ/S1364-6613(07)00212-4.html:text/html}
