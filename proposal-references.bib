@article{Wyble2000,
  title = {Size of {CA}1-Evoked Synaptic Potentials Is Related to Theta Rhythm Phase in Rat Hippocampus},
  volume = {83},
  rights = {Copyright © 2000 The American Physiological Society},
  issn = {0022-3077, 1522-1598},
  url = {http://jn.physiology.org/content/83/4/2138},
  abstract = {Cholinergic and GABAergic neurons projecting to the hippocampus fire with specific phase relations to theta rhythm oscillations in the electroencephalogram (EEG). To determine if this phasic input has an impact on synaptic transmission within the hippocampus, we recorded evoked population excitatory postsynaptic potential (EPSPs) during different phases of theta rhythm by using techniques similar to those described in Rudell and Fox. Synaptic potentials elicited by stimulation of region CA3 of the contralateral hippocampus were recorded in region CA1 and CA3. In these experiments, the initial slope of evoked potentials showed a change in magnitude during different phases of the theta rhythm recorded in the dentate fissure, with individual trials showing an average of 9.5\% change in slope of potentials, and the average across all experiments showing a change of 7.8\%. Evoked potentials were maximal 18° after the positive peak of the dentate fissure theta EEG. These potentials were also smaller by 18.2\% during theta as opposed to non-theta states. Phasic changes in modulation of synaptic transmission could contribute to phase precession of hippocampal place cells and could enhance storage of new sequences of activity as demonstrated by computational models.},
  timestamp = {2015-01-28 19:47:50},
  eprinttype = {pmid},
  eprint = {10758123},
  number = {4},
  journaltitle = {Journal of Neurophysiology},
  author = {Wyble, Bradley P. and Linster, Christiane and Hasselmo, Michael E.},
  urldate = {2015-01-28},
  date = {2000-04-01},
  pages = {2138--2144},
  langid = {english},
  note = {Cholinergic and GABAergic neurons projecting to the hippocampus fire with specific phase relations to theta rhythm oscillations in the electroencephalogram (EEG). To determine if this phasic input has an impact on synaptic transmission within the hippocampus, we recorded evoked population excitatory postsynaptic potential (EPSPs) during different phases of theta rhythm by using techniques similar to those described in Rudell and Fox. Synaptic potentials elicited by stimulation of region CA3 of the contralateral hippocampus were recorded in region CA1 and CA3. In these experiments, the initial slope of evoked potentials showed a change in magnitude during different phases of the theta rhythm recorded in the dentate fissure, with individual trials showing an average of 9.5\% change in slope of potentials, and the average across all experiments showing a change of 7.8\%. Evoked potentials were maximal 18° after the positive peak of the dentate fissure theta EEG. These potentials were also smaller by 18.2\% during theta as opposed to non-theta states. Phasic changes in modulation of synaptic transmission could contribute to phase precession of hippocampal place cells and could enhance storage of new sequences of activity as demonstrated by computational models.},
  file = {Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/9NWRTM2C/2138.html:text/html;Wyble et al_2000_Size of CA1-Evoked Synaptic Potentials Is Related to Theta Rhythm Phase in Rat.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/RHFH9ZFD/Wyble et al_2000_Size of CA1-Evoked Synaptic Potentials Is Related to Theta Rhythm Phase in Rat.pdf:application/pdf}
