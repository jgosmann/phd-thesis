  urldate = {2015-01-30},
  date = {2007-11-01},
  pages = {745--757},
  langid = {english},
  file = {Leutgeb_Leutgeb_2007_Pattern separation, pattern completion, and new neuronal codes within a.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/5BKWZARI/Leutgeb_Leutgeb_2007_Pattern separation, pattern completion, and new neuronal codes within a.pdf:application/pdf;Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/PTE85ECB/745.html:text/html}
