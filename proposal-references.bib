  author = {Owen, Adrian M. and Sahakian, Barbara J. and Semple, James and Polkey, Charles E. and Robbins, Trevor W.},
  urldate = {2015-02-03},
  date = {1995-01},
  pages = {1--24},
  keywords = {Alzheimer's,frontal lobe,Parkinson's,spatial memory,Temporal Lobe,Visual Memory},
  file = {Owen et al_1995_Visuo-spatial short-term recognition memory and learning after temporal lobe.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/TEA4CQKR/Owen et al_1995_Visuo-spatial short-term recognition memory and learning after temporal lobe.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/ASEC9QPX/002839329400098A.html:text/html}
