  urldate = {2015-01-05},
  date = {2011-01-10},
  pages = {504--514},
  langid = {english},
  file = {Mednick et al_2011_An opportunistic theory of cellular and systems consolidation.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/G8KTKG7N/Mednick et al_2011_An opportunistic theory of cellular and systems consolidation.pdf:application/pdf;Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/8DSPPQ3G/S0166-2236(11)00091-9.html:text/html}
