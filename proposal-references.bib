  pages = {505--510},
  keywords = {animal learning,Computational models,Hippocampus,memory,Neocortex,Recognition memory},
  file = {O'Reilly_Norman_2002_Hippocampal and neocortical contributions to memory.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/VSF93SXI/O'Reilly_Norman_2002_Hippocampal and neocortical contributions to memory.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/SVUJVUI3/S1364661302020053.html:text/html}
