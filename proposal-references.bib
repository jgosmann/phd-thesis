  urldate = {2015-01-05},
  date = {2002-08-01},
  pages = {6830--6834},
  langid = {english},
  keywords = {Declarative memory,EEG,human,Learning,sleep,sleep spindles},
  file = {Gais et al_2002_Learning-Dependent Increases in Sleep Spindle Density.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/EPN2JNNI/Gais et al_2002_Learning-Dependent Increases in Sleep Spindle Density.pdf:application/pdf;Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/Q258SBDV/6830.html:text/html}
