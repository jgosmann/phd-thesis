}

@article{Lee2005,
  title = {Perceptual deficits in amnesia: challenging the medial temporal lobe ‘mnemonic’ view},
  volume = {43},
  issn = {0028-3932},
  url = {http://www.sciencedirect.com/science/article/pii/S0028393204001939},
  doi = {10.1016/j.neuropsychologia.2004.07.017},
  shorttitle = {Perceptual deficits in amnesia},
  abstract = {Recent animal studies suggest that the medial temporal lobe (MTL), which is thought to subserve memory exclusively, may support non-mnemonic perceptual processes, with the hippocampus and perirhinal cortex contributing to spatial and object perception, respectively. There is, however, no support for this view in humans, with human MTL lesions causing prominent memory deficits in the context of apparently normal perception. We assessed visual discrimination in amnesic cases to reveal that while selective hippocampal damaged patients could discriminate faces, objects, abstract art and colour, they were significantly poorer in discriminating spatial scenes. By contrast, patients with MTL damage, including perirhinal cortex, were significantly impaired in discriminating scenes, faces, and to a lesser extent objects, with relatively intact discrimination of art and colour. These novel observations imply that the human MTL subserves both perceptual and mnemonic functions, with the hippocampus and perirhinal cortex playing distinct roles in spatial and object discrimination, respectively.},
  timestamp = {2015-02-03 20:43:48},
  number = {1},
  journaltitle = {Neuropsychologia},
  shortjournal = {Neuropsychologia},
  author = {Lee, Andy C. H. and Bussey, Timothy J. and Murray, Elisabeth A. and Saksida, Lisa M. and Epstein, Russell A. and Kapur, Narinder and Hodges, John R. and Graham, Kim S.},
  urldate = {2015-02-03},
  date = {2005},
  pages = {1--11},
  keywords = {Hippocampus,memory,perception,Perirhinal cortex,Visual discrimination},
  file = {Lee et al_2005_Perceptual deficits in amnesia.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/24HGHVRR/Lee et al_2005_Perceptual deficits in amnesia.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/2W2QBVJS/S0028393204001939.html:text/html}
}

@article{Murray1999,
  title = {Perceptual–mnemonic functions of the perirhinal cortex},
  volume = {3},
  issn = {1364-6613},
  url = {http://www.sciencedirect.com/science/article/pii/S1364661399013030},
  doi = {10.1016/S1364-6613(99)01303-0},
  abstract = {It is widely acknowledged that the perirhinal cortex, located in the ventromedial aspect of the temporal lobe, is essential for certain types of memory in macaque monkeys. For example, removal of the perirhinal cortex yields severe impairments on tests of stimulus recognition and stimulus–stimulus association. There is considerable disagreement, however, about the most accurate way to characterize the function of the perirhinal cortex; some views emphasize a role in perception whereas others posit a role exclusively in declarative memory. In this article, we review recent findings from anatomical, physiological and ablation studies in monkeys, and discuss related findings obtained in humans, in an attempt to identify not only the cognitive functions of the perirhinal cortex, but also the implications of these findings for theoretical views concerning the organization of memory.},
  timestamp = {2015-02-03 20:44:14},
  number = {4},
  journaltitle = {Trends in Cognitive Sciences},
  shortjournal = {Trends in Cognitive Sciences},
  author = {Murray, Elisabeth A. and Bussey, Timothy J.},
  urldate = {2015-02-03},
  date = {1999-04-01},
  pages = {142--151},
  keywords = {Hippocampus,inferotemporal cortex,object identification,parahippocampal cortex,Perirhinal cortex,Recognition memory,Visual discrimination},
  file = {Murray_Bussey_1999_Perceptual–mnemonic functions of the perirhinal cortex.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/G32B5BDZ/Murray_Bussey_1999_Perceptual–mnemonic functions of the perirhinal cortex.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/EQNBUNJR/S1364661399013030.html:text/html}
}

