  urldate = {2015-01-21},
  date = {1987-03-17},
  pages = {280--287},
  keywords = {Cell count,Neuronal cell death,Neuronal number,Strain difference},
  file = {Boss et al_1987_On the numbers of neurons on fields CA1 and CA3 of the hippocampus of.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/9XGCWN22/Boss et al_1987_On the numbers of neurons on fields CA1 and CA3 of the hippocampus of.pdf:application/pdf;ScienceDirect Snapshot:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/J6K5T4SR/0006899387907931.html:text/html}
