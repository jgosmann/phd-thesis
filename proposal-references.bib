  keywords = {Aged,amnesia,Discrimination (Psychology),Female,Humans,Limbic System,Magnetic Resonance Imaging,Male,Middle Aged,Pattern Recognition, Visual,Photic Stimulation,reaction time,Reference Values,Temporal Lobe},
  file = {Buffalo et al_1998_The human perirhinal cortex and recognition memory.pdf:/home/jgosmann/.zotero/zotero/g4y0i8zx.default/zotero/storage/P2BFJ32B/Buffalo et al_1998_The human perirhinal cortex and recognition memory.pdf:application/pdf}
